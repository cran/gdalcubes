{
  "description" : "Preliminary collection format for MAXAR open data, visual only (under development)",
  "references" : "",
  "tags" : [""],
  "pattern" : ".+-visual\\.tif$",
  "images" : {
    "pattern" : ".*[/\\\\](.*[/\\\\].*[/\\\\].*)-visual\\.tif$"
  },
  "datetime" : {
    "pattern" : ".*[/\\\\]([0-9]{4}-[0-9]{2}-[0-9]{2})[/\\\\].*-visual\\.tif$",
    "format" : "%Y-%m-%d"
  },
  "bands": {
    "R" : {
      "pattern" : ".+-visual\\.tif$",
      "band": 1
    },
    "G" : {
      "pattern" : ".+-visual\\.tif$",
      "band": 2
    },
    "B" : {
      "pattern" : ".+-visual\\.tif$",
      "band": 3
    }
  }
}